{
  "name": "mappedlove",
  "version": "1.0.0",
  "description": "Web app made for couples. Mark the places you have visited together upload images and preserve your special moments forever.",
  "author": "Yizack Rangel",
  "type": "module",
  "license": "MIT",
  "homepage": "https://mappedlove.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yizack/mappedlove"
  },
  "bugs": {
    "url": "https://github.com/Yizack/mappedlove/issues"
  },
  "scripts": {
    "build": "npm run docs:build && nuxt build",
    "dev": "nuxt dev --port=5173 --host",
    "generate": "nuxt generate",
    "preview": "npx nuxthub preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "db:migrate": "drizzle-kit generate --config=.config/drizzle.ts",
    "lint": "pnpm lint:eslint && pnpm lint:stylelint",
    "lint:fix": "pnpm lint:eslint --fix && pnpm lint:stylelint --fix",
    "lint:eslint": "eslint --config .config/eslint.mjs",
    "lint:eslint:inspect": "npx @eslint/config-inspector --config .config/eslint.mjs",
    "lint:stylelint": "stylelint \"**/*.{css,scss,sass}\" --config .config/stylelint.json",
    "docs:dev": "vitepress dev docs",
    "docs:build": "(git fetch --unshallow || git fetch --all) && vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "worker:dev": "npx wrangler dev --config worker/wrangler.json",
    "worker:deploy": "npx wrangler deploy --config worker/wrangler.json",
    "test:types": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250327.0",
    "@dargmuesli/nuxt-cookie-control": "^8.5.5",
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/solar": "^1.2.2",
    "@iconify-json/tabler": "^1.2.17",
    "@nuxt/eslint": "^1.2.0",
    "@nuxt/icon": "^1.11.0",
    "@nuxthub/core": "^0.8.21",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/sitemap": "^7.2.9",
    "@nuxtjs/turnstile": "^0.9.16",
    "@paddle/paddle-js": "^1.4.0",
    "@paddle/paddle-node-sdk": "^2.5.1",
    "@types/bootstrap": "^5.2.10",
    "@types/leaflet": "^1.9.17",
    "@vue-email/components": "^0.0.21",
    "@vue-email/render": "^0.0.9",
    "@vuepic/vue-datepicker": "^11.0.2",
    "@yeger/vue-masonry-wall": "^5.0.18",
    "bootstrap": "^5.3.3",
    "drizzle-kit": "^0.30.6",
    "drizzle-orm": "^0.41.0",
    "eslint": "^9.23.0",
    "html-to-text": "^9.0.5",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^4.2.0",
    "nuxt": "^3.16.1",
    "nuxt-auth-utils": "^0.5.17",
    "nuxt-mailchannels": "^0.2.1",
    "nuxt-time": "^1.0.3",
    "nuxt-twemoji": "3.6.3",
    "nuxt-webhook-validators": "^0.1.11",
    "sass": "^1.86.0",
    "stylelint": "^16.17.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.8.2",
    "vitepress": "^1.6.3",
    "vue-draggable-next": "^2.2.1",
    "vue-tsc": "^2.2.8",
    "wrangler": "^4.5.1",
    "zod": "^3.24.2"
  },
  "packageManager": "pnpm@10.7.0"
}
