{
  "name": "mappedlove",
  "version": "1.0.0",
  "description": "Web app made for couples. Mark the places you have visited together upload images and preserve your special moments forever.",
  "author": "Yizack Rangel",
  "type": "module",
  "license": "MIT",
  "homepage": "https://mappedlove.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yizack/mappedlove"
  },
  "bugs": {
    "url": "https://github.com/Yizack/mappedlove/issues"
  },
  "scripts": {
    "build": "npm run email:templates && nuxt build",
    "dev": "nuxt dev --port=5173 --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "db:migrate": "drizzle-kit generate:sqlite --config=.config/drizzle.json",
    "email:templates": "npx vue-email export --out=./server/email-templates && tsx ./scripts/emailTemplates.ts",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint --fix --config .config/eslint.mjs",
    "lint:stylelint": "stylelint \"**/*.css\" --fix --config .config/stylelint.json",
    "cf:dev": "npm run build && wrangler pages dev dist"
  },
  "devDependencies": {
    "@dargmuesli/nuxt-cookie-control": "^8.2.3",
    "@nuxt/eslint": "^0.3.12",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/sitemap": "^5.1.5",
    "@nuxtjs/turnstile": "^0.8.0",
    "@paddle/paddle-js": "^1.0.3",
    "@paddle/paddle-node-sdk": "^1.3.0",
    "@paypal/paypal-js": "^8.0.5",
    "@types/bootstrap": "^5.2.10",
    "@types/leaflet": "^1.9.12",
    "@types/mustache": "^4.2.5",
    "@vuepic/vue-datepicker": "^8.5.1",
    "@yeger/vue-masonry-wall": "^5.0.10",
    "better-sqlite3": "^9.6.0",
    "bootstrap": "^5.3.3",
    "drizzle-kit": "^0.20.18",
    "drizzle-orm": "^0.30.10",
    "eslint": "^9.2.0",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^3.11.1",
    "mustache": "^4.2.0",
    "nodemailer": "^6.9.13",
    "nuxt": "^3.11.2",
    "nuxt-icon": "^0.6.10",
    "nuxt-twemoji": "^3.5.0",
    "stylelint": "^16.5.0",
    "stylelint-config-standard": "^36.0.0",
    "tsx": "^4.9.4",
    "typescript": "^5.4.5",
    "vue-draggable-next": "^2.2.1",
    "vue-email": "^0.8.11",
    "wrangler": "^3.55.0",
    "zod": "^3.23.8"
  }
}
