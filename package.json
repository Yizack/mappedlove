{
  "name": "mappedlove",
  "version": "1.0.0",
  "description": "Yizack nuxt template",
  "author": "Yizack Rangel",
  "type": "module",
  "homepage": "https://github.com/Yizack/mappedlove",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yizack/mappedlove"
  },
  "bugs": {
    "url": "https://github.com/Yizack/mappedlove/issues"
  },
  "scripts": {
    "build": "pnpm email:templates && nuxt build",
    "dev": "nuxt dev --port=5173",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:migrate": "drizzle-kit generate:sqlite",
    "email:templates": "tsc ./scripts/emailTemplates.ts --module nodenext && node ./scripts/emailTemplates.js",
    "lint": "pnpm lint:eslint && pnpm lint:stylelint",
    "lint:eslint": "eslint --ext .ts,.js,.vue --fix --ignore-path .gitignore .",
    "lint:stylelint": "stylelint \"**/*.css\""
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxtjs/turnstile": "^0.5.0",
    "@types/bootstrap": "^5.2.6",
    "@types/leaflet": "^1.9.3",
    "better-sqlite3": "^8.5.0",
    "drizzle-kit": "^0.19.12",
    "drizzle-orm": "^0.27.2",
    "eslint": "^8.46.0",
    "nodemailer": "^6.9.4",
    "nuxt": "^3.6.5",
    "nuxt-icon": "^0.4.2",
    "nuxt-twemoji": "^3.3.1",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.1.6",
    "vue-draggable-next": "^2.2.1",
    "wrangler": "3.4.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.1",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^3.8.0",
    "mustache": "^4.2.0"
  }
}
