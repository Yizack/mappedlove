{
  "private": true,
  "name": "mappedlove",
  "version": "1.0.0",
  "description": "Web app made for couples. Mark the places you have visited together upload images and preserve your special moments forever.",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Yizack Rangel",
    "email": "yizackr@gmail.com",
    "url": "https://yizack.com"
  },
  "homepage": "https://mappedlove.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yizack/mappedlove.git"
  },
  "bugs": "https://github.com/Yizack/mappedlove/issues",
  "scripts": {
    "build": "npm run docs:build && nuxt build",
    "dev": "nuxt dev --port=5173 --public",
    "dev:test": "nuxt dev --port=3000 --dotenv=.env.test --envName=test",
    "generate": "nuxt generate",
    "preview": "wrangler d1 migrations apply DB --local && wrangler dev",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "analyze": "nuxt analyze",
    "db:migrate": "drizzle-kit generate --config=.config/drizzle.ts",
    "lint": "pnpm lint:eslint && pnpm lint:stylelint",
    "lint:fix": "pnpm lint:eslint --fix && pnpm lint:stylelint --fix",
    "lint:eslint": "eslint --config .config/eslint.mjs",
    "lint:eslint:inspect": "pnpx @eslint/config-inspector --config .config/eslint.mjs",
    "lint:stylelint": "stylelint \"**/*.{css,scss,sass}\" --config .config/stylelint.json",
    "docs:dev": "vitepress dev docs",
    "docs:build": "(git fetch --unshallow -q || git fetch --all -q) && vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "pnpx concurrently --raw --kill-others --success first \"pnpm dev:test\" \"pnpm test:wait && pnpm test:vitest\"",
    "test:wait": "pnpx wait-on http://localhost:3000 && pnpx wait-on http-get://localhost:3000/_nitro/tasks/seed",
    "test:vitest": "vitest run --reporter=verbose --config=.config/vitest.ts",
    "test:types": "vue-tsc -b --noEmit"
  },
  "dependencies": {
    "@dargmuesli/nuxt-cookie-control": "^9.1.17",
    "@iconify-json/ion": "^1.2.6",
    "@iconify-json/logos": "^1.2.10",
    "@iconify-json/solar": "^1.2.5",
    "@iconify-json/tabler": "^1.2.28",
    "@libsql/client": "^0.17.0",
    "@nuxt/icon": "^2.2.1",
    "@nuxthub/core": "^0.10.6",
    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/mdc": "^0.20.1",
    "@nuxtjs/sitemap": "^7.6.0",
    "@nuxtjs/turnstile": "^1.1.1",
    "@paddle/paddle-js": "^1.6.2",
    "@paddle/paddle-node-sdk": "^3.6.0",
    "@vuepic/vue-datepicker": "^12.1.0",
    "@yeger/vue-masonry-wall": "^6.1.0",
    "bootstrap": "^5.3.8",
    "drizzle-kit": "^0.31.9",
    "drizzle-orm": "^0.45.1",
    "leaflet": "^2.0.0-alpha.1",
    "leaflet-geosearch": "^4.2.2",
    "nuxt": "^4.3.1",
    "nuxt-auth-utils": "^0.5.29",
    "nuxt-email-renderer": "^2.5.3",
    "nuxt-mailchannels": "^0.3.13",
    "nuxt-twemoji": "^4.0.1",
    "nuxt-webhook-validators": "^0.2.7",
    "sass": "^1.97.3",
    "vitepress": "^2.0.0-alpha.16",
    "vue-draggable-plus": "^0.6.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.15.2",
    "@nuxt/test-utils": "^4.0.0",
    "@types/bootstrap": "^5.2.10",
    "@types/leaflet": "^1.9.21",
    "eslint": "^10.0.2",
    "happy-dom": "^20.7.0",
    "stylelint": "^17.4.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.5",
    "wrangler": "^4.69.0"
  },
  "packageManager": "pnpm@10.30.3"
}
