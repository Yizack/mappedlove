{
  "name": "mappedlove",
  "version": "1.0.0",
  "description": "Web app made for couples. Mark the places you have visited together upload images and preserve your special moments forever.",
  "author": "Yizack Rangel",
  "type": "module",
  "license": "MIT",
  "homepage": "https://mappedlove.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yizack/mappedlove"
  },
  "bugs": {
    "url": "https://github.com/Yizack/mappedlove/issues"
  },
  "scripts": {
    "build": "npm run email:templates && npm run docs:build && nuxt build",
    "dev": "nuxt dev --port=5173 --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "db:migrate": "drizzle-kit generate --config=.config/drizzle.json",
    "email:templates": "npx vue-email export --out=./server/email-templates && tsx ./scripts/emailTemplates.ts",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint --fix --config .config/eslint.mjs",
    "lint:stylelint": "stylelint \"**/*.css\" --fix --config .config/stylelint.json",
    "cf:dev": "npm run build && wrangler pages dev dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "(git fetch --unshallow || git fetch --all) && vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "devDependencies": {
    "@dargmuesli/nuxt-cookie-control": "^8.4.2",
    "@nuxt/eslint": "^0.3.13",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/sitemap": "^5.2.0",
    "@nuxtjs/turnstile": "^0.8.0",
    "@paddle/paddle-js": "^1.2.0",
    "@paddle/paddle-node-sdk": "^1.3.0",
    "@paypal/paypal-js": "^8.1.0",
    "@types/bootstrap": "^5.2.10",
    "@types/leaflet": "^1.9.12",
    "@types/mustache": "^4.2.5",
    "@vuepic/vue-datepicker": "^8.8.0",
    "@yeger/vue-masonry-wall": "^5.0.10",
    "better-sqlite3": "^11.0.0",
    "bootstrap": "^5.3.3",
    "drizzle-kit": "^0.22.7",
    "drizzle-orm": "^0.31.2",
    "eslint": "^9.5.0",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^4.0.0",
    "mustache": "^4.2.0",
    "nodemailer": "^6.9.13",
    "nuxt": "^3.11.2",
    "nuxt-icon": "^0.6.10",
    "nuxt-twemoji": "^3.5.0",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.0",
    "tsx": "^4.15.5",
    "typescript": "^5.4.5",
    "vitepress": "^1.2.3",
    "vue-draggable-next": "^2.2.1",
    "vue-email": "^0.8.11",
    "wrangler": "^3.60.3",
    "zod": "^3.23.8"
  },
  "packageManager": "pnpm@9.3.0"
}
