{
  "private": true,
  "name": "mappedlove",
  "version": "1.0.0",
  "description": "Web app made for couples. Mark the places you have visited together upload images and preserve your special moments forever.",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Yizack Rangel",
    "email": "yizackr@gmail.com",
    "url": "https://yizack.com"
  },
  "homepage": "https://mappedlove.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yizack/mappedlove.git"
  },
  "bugs": "https://github.com/Yizack/mappedlove/issues",
  "scripts": {
    "build": "npm run docs:build && nuxt build",
    "dev": "nuxt dev --port=5173 --host",
    "dev:test": "nuxt dev --port=3000 --dotenv=.env.test --envName=test",
    "generate": "nuxt generate",
    "preview": "wrangler d1 migrations apply DB --local && wrangler dev",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "analyze": "nuxt analyze",
    "db:migrate": "drizzle-kit generate --config=.config/drizzle.ts",
    "lint": "pnpm lint:eslint && pnpm lint:stylelint",
    "lint:fix": "pnpm lint:eslint --fix && pnpm lint:stylelint --fix",
    "lint:eslint": "eslint --config .config/eslint.mjs",
    "lint:eslint:inspect": "pnpx @eslint/config-inspector --config .config/eslint.mjs",
    "lint:stylelint": "stylelint \"**/*.{css,scss,sass}\" --config .config/stylelint.json",
    "docs:dev": "vitepress dev docs",
    "docs:build": "(git fetch --unshallow || git fetch --all) && vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "pnpx concurrently --raw --kill-others --success first \"pnpm dev:test\" \"pnpm test:wait && pnpm test:vitest\"",
    "test:wait": "pnpx wait-on http://localhost:3000 && pnpx wait-on http-get://localhost:3000/_nitro/tasks/seed",
    "test:vitest": "vitest run --reporter=verbose --config=.config/vitest.ts",
    "test:types": "vue-tsc -b --noEmit"
  },
  "devDependencies": {
    "@dargmuesli/nuxt-cookie-control": "^9.1.5",
    "@iconify-json/ion": "^1.2.6",
    "@iconify-json/logos": "^1.2.9",
    "@iconify-json/solar": "^1.2.4",
    "@iconify-json/tabler": "^1.2.23",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/icon": "^2.0.0",
    "@nuxt/test-utils": "npm:@nuxt/test-utils-nightly@3.19.3-20250916-214026-1028863",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/mdc": "^0.17.4",
    "@nuxtjs/sitemap": "^7.4.7",
    "@nuxtjs/turnstile": "^1.1.0",
    "@paddle/paddle-js": "^1.4.2",
    "@paddle/paddle-node-sdk": "^3.3.0",
    "@types/bootstrap": "^5.2.10",
    "@types/leaflet": "^1.9.20",
    "@vuepic/vue-datepicker": "^11.0.2",
    "@yeger/vue-masonry-wall": "^5.1.1",
    "bootstrap": "^5.3.8",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "eslint": "^9.37.0",
    "happy-dom": "^19.0.2",
    "leaflet": "^2.0.0-alpha.1",
    "leaflet-geosearch": "^4.2.2",
    "nuxt": "^4.1.3",
    "nuxt-auth-utils": "^0.5.25",
    "nuxt-email-renderer": "^0.0.21",
    "nuxt-mailchannels": "^0.3.6",
    "nuxt-twemoji": "^3.7.4",
    "nuxt-webhook-validators": "^0.2.5",
    "sass": "^1.93.2",
    "stylelint": "^16.25.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "typescript": "^5.9.3",
    "vitepress": "^2.0.0-alpha.12",
    "vitest": "^3.2.4",
    "vue-draggable-plus": "^0.6.0",
    "vue-tsc": "^3.1.1",
    "wrangler": "^4.42.0",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.18.1"
}
