{
  "name": "mappedlove",
  "version": "1.0.0",
  "description": "Web app made for couples. Mark the places you have visited together upload images and preserve your special moments forever.",
  "author": "Yizack Rangel",
  "type": "module",
  "license": "MIT",
  "homepage": "https://mappedlove.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yizack/mappedlove"
  },
  "bugs": {
    "url": "https://github.com/Yizack/mappedlove/issues"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port=5173 --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "db:migrate": "drizzle-kit generate:sqlite",
    "email:templates": "tsx ./scripts/emailTemplates.ts",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint --ext .ts,.js,.vue --fix --ignore-path .gitignore .",
    "lint:stylelint": "stylelint \"**/*.css\" --fix",
    "dev:cf": "npm run build && npx wrangler dev dist/_worker.js"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/sitemap": "^5.1.1",
    "@nuxtjs/turnstile": "^0.6.3",
    "@paddle/paddle-js": "^1.0.2",
    "@paddle/paddle-node-sdk": "^1.0.1",
    "@paypal/paypal-js": "^8.0.2",
    "@types/bootstrap": "^5.2.10",
    "@types/leaflet": "^1.9.8",
    "@vue-email/nuxt": "^0.8.19",
    "@vuepic/vue-datepicker": "^8.2.0",
    "@yeger/vue-masonry-wall": "^5.0.8",
    "better-sqlite3": "^9.4.3",
    "bootstrap": "^5.3.3",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.29.4",
    "eslint": "^8.57.0",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^3.11.1",
    "nodemailer": "^6.9.11",
    "nuxt": "^3.10.3",
    "nuxt-icon": "^0.6.9",
    "nuxt-twemoji": "^3.4.5",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vue": "^3.4.21",
    "vue-draggable-next": "^2.2.1",
    "wrangler": "^3.31.0",
    "zod": "^3.22.4"
  }
}
